		"prettier-plugin-toml": "^2.0.6"
	},
	"prettier": {
		"useTabs": true,
		"plugins": [
			"prettier-plugin-toml"
		],
		"overrides": [
			{
				"files": "*.md",
				"options": {
					"useTabs": false
				}
			},
			{
				"files": "*.toml",
				"options": {
					"printWidth": 80,
					"tabWidth": 2,
					"useTabs": true
				}
			}
		]
	}
